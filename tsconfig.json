{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "importsNotUsedAsValues": "error"
  },
  "paths": {
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"]
  },
  "plugins": [
    {
      "name": "@astrojs/ts-plugin"
    }
  ]
}
